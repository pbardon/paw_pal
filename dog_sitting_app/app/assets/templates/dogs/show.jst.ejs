<div class="dogShowInfo">

  <h1><%= dog.escape('name') %></h1>

<ul>
  <li>Age: <%= dog.escape('age') %></li>
  <li>Description:
    <p class="dog-description"><%= dog.escape('description') %></p>
  </li>
  <li>Size: <%= dog.escape('size') %></li>
  <li>
    <a href= "#/api/dogs/<%= dog.escape('id') %>/edit"> Edit Dog Information </a>
  </li>
</ul>

</div>
