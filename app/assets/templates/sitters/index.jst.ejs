<!-- <div class="sitters-feed"> -->

<div class="row">
<div class="col-xs-8">
  <% sitters.each(function(sitter){ %>
    <div class="row">
      <!-- <div class="sitter-info"> -->
      <div class="sitterItem col-xs-12">
        <div class="row">
          <div class = 'col-xs-8'>
            <h4><a href="#/sitters/<%= sitter.get('id') %>"><%= sitter.escape('sitter_name') %></a></h4>
            <ul>
              <li><p><%= sitter.escape('description') %></p></li>
              <li>Cost per Night:<%= sitter.escape('price') %></li>
          </div>
          <div class="sitterIndexImg col-xs-2 col-xs-offset-2">
            <img src="<%= sitter.get('sitter_photo_small') %>">
          </div>
        </div>
      </div>

      <!-- <div class="sitter-pic"> -->

    </div>
  <% }); %>
</div>

<!-- </div> -->

  <div class="map-wrapper col-xs-4">

  <div id="map-canvas" style="width:400px;height:400px;">

  </div>
</div>
